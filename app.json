{
  "expo": {
    "name": "Receipt Splitter",
    "slug": "receipt-splitter",
    "scheme": "receipt-splitter",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.Sukhrob.receiptsplitter",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c49eb3a1-445f-40a7-ae06-0a54e8daf1dc"
      }
    }
  }
}
